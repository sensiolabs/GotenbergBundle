{
    "name": "sensiolabs/gotenberg-bundle",
    "description": "Gotenberg support for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Sensiolabs\\GotenbergBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sensiolabs\\GotenbergBundle\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Steven Renaux",
            "email": "steven.renaux@sensiolabs.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "symfony/config": "^6.3",
        "symfony/dependency-injection": "^6.3",
        "symfony/http-kernel": "^6.3",
        "symfony/mime": "^6.3",
        "symfony/string": "^6.3",
        "symfony/http-client": "^6.3",
        "symfony/filesystem": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "symfony/framework-bundle": "^6.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-symfony": "^1.3",
        "symfony/twig-bundle": "^6.3"
    },
    "suggest": {
        "symfony/twig-bundle": "Allow you to use twig to add template into PDF"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
